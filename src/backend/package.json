{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "build:api-functions": "tsc && node_modules/.bin/netlify-lambda build dist",
    "postbuild": "cp ./index.html ./dist/",
    "prestart": "yarn build",
    "watch": "tsc -w",
    "dev": "concurrently --kill-others \"yarn watch\" \"nodemon .\"",
    "dev:serve": "tsc && node_modules/.bin/netlify-lambda serve dist",
    "start": "node ."
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "netlify-lambda": "^2.0.15",
    "serverless-http": "^3.0.2"
  }
}
